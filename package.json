{
  "name": "activity-tracker-nodejs",
  "version": "1.0.0",
  "description": "Activity tracker for starting and stopping actvities",
  "main": "app.ts",
  "author": "Yazan Alkhatib",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p .",
    "dev": "nodemon --exec ts-node src/app.ts",
    "start": "ts-node dist/src/app.js",
    "migrate": "knex --knexfile=src/knexfile.ts migrate:latest",
    "rollback": "knex --knexfile=src/knexfile.ts migrate:rollback"
  },
  "_moduleAliases": {
    "lib/*": "src/lib/*",
    "lib": "src/lib/index.ts",
    "models/*": "src/models/*",
    "models": "src/models/index.ts",
    "controllers/*": "src/controllers/*",
    "controllers": "src/controllers/index.ts",
    "services/*": "src/services/*",
    "services": "src/services/index.ts",
    "middlewares/*": "src/middlewares/*",
    "middlewares": "src/middlewares/index.ts"
  },
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.95.4",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.7",
    "objection": "^2.2.15",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {}
}
